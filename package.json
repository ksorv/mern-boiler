{
  "name": "mern-boiler",
  "version": "1.0.0",
  "description": "MERN Stack boiler for faster usage",
  "main": "index.js",
  "scripts": {
    "start:backend": "node server/index.js",
    "start:frontend": "npm run start --prefix client",
    "ready": "concurrently \"npm install\" \"npm install --prefix client\"",
    "dev": "concurrently \"npm run start:*\" --kill-others"
